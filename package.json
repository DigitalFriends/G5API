{
  "name": "G5APIv1",
  "version": "0.0.1",
  "private": true,
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "homepage": "https://github.com/phlexplexico/G5API/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/phlexplexico/G5API.git"
  },
  "author": {
    "name": "Phlex Plexico",
    "email": "flexing@phlexplexi.co",
    "url": "https://phlexplexi.co"
  },
  "engines": {
    "node": ">=11.15.0"
  },
  "directories": {
    "docs": "./docs/",
    "bin": "./bin/",
    "routes": "./routes/",
    "config": "./config"
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "doc": "jsdoc -c jsdoc.conf.json",
    "docclean": "rm -rf ./docs",
    "migrate-dev": "node ./node_modules/db-migrate/bin/db-migrate --config config/development.json",
    "migrate-prod": "node ./node_modules/db-migrate/bin/db-migrate --env prod --config config/production.json",
    "migrate-dev-upgrade": "node ./node_modules/db-migrate/bin/db-migrate --config config/development.json up:dev",
    "migrate-dev-downgrade": "node ./node_modules/db-migrate/bin/db-migrate --config config/development.json down:dev",
    "migrate-prod-upgrade": "node ./node_modules/db-migrate/bin/db-migrate --env prod --config config/production.json up:prod",
    "migrate-prod-downgrade": "node ./node_modules/db-migrate/bin/db-migrate --env prod --config config/production.json down:prod"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "config": "^3.2.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.6",
    "db-migrate-mysql": "^1.1.10",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-bearer-token": "^2.4.0",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "http-errors": "~1.6.3",
    "jsdoc": "^3.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-steam": "^1.0.11",
    "randomstring": "^1.1.5",
    "rcon-srcds": "^1.0.0"
  }
}
