{
  "name": "G5APIv1",
  "version": "0.0.1",
  "private": true,
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "homepage": "https://github.com/phlexplexico/G5API/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/phlexplexico/G5API.git"
  },
  "author": {
    "name": "Phlex Plexico",
    "email": "flexing@phlexplexi.co",
    "url": "https://phlexplexi.co"
  },
  "engines": {
    "node": ">=12"
  },
  "directories": {
    "docs": "./docs/",
    "bin": "./bin/",
    "routes": "./routes/",
    "config": "./config"
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "doc": "jsdoc -c jsdoc.conf.json",
    "docclean": "rm -rf ./docs",
    "migrate-create-dev": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --config config/development.json db:create get5test",
    "migrate-create-prod": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --env prod --config config/production.json db:create get5",
    "migrate-dev-downgrade": "db-migrate --config config/development.json down:dev",
    "migrate-dev-upgrade": "db-migrate --config config/development.json up:dev",
    "migrate-drop-dev": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --config config/development.json db:drop get5test",
    "migrate-drop-prod": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --env prod --config config/production.json db:drop",
    "migrate-prod-upgrade": "db-migrate --env prod --config config/production.json up:prod",
    "migrate-prod-downgrade": "db-migrate --env prod --config config/production.json down:prod",
    "migrate-session-create-dev": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --config config/development.json db:create get5test_session",
    "migrate-session-drop-dev": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --config config/development.json db:drop get5test_session",
    "migrate-session-create-prod": "MYSQL_FLAGS=\"-CONNECT_WITH_DB\" db-migrate --env prod --config config/production.json db:create get5session",
    "prod": "NODE_ENV=production && yarn migrate-session-create-prod && yarn migrate-create-prod && yarn migrate-prod-upgrade",
    "test": "NODE_ENV=development && yarn migrate-session-drop-dev && yarn migrate-session-create-dev && yarn migrate-drop-dev && yarn migrate-create-dev && yarn migrate-dev-upgrade && jest --testTimeout=10000 --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "config": "^3.2.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.6",
    "db-migrate-mysql": "^2.1.1",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-mysql-session": "^2.1.4",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "http-errors": "~1.7.3",
    "jsdoc": "^3.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-steam": "^1.0.14",
    "randomstring": "^1.1.5",
    "rcon-srcds": "^1.0.0"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "supertest": "^4.0.2"
  }
}
